{# CDN constants #}
{% set bootstrap_css_cdn = 'https://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/css/bootstrap.min.css' %}
{% set bootstrap_js_cdn = 'https://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/js/bootstrap.min.js' %}
{% set jquery_cdn = 'https://ajax.googleapis.com/ajax/libs/jquery/1.11.3/jquery.min.js' %}

<!DOCTYPE html>
<html lang="es">
  <head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>{% if block('title') %}{% block title %}{% endblock %} - {% endif %}AMCB - Asociación de Música de Cámara de Bilbao</title>
    {% block default_stylesheets %}{% endblock %}
    {% block default_javascripts %}{% endblock %}
    <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
    {% block extra_metatags %}{% endblock %}
  </head>
  <body>
    {% block body %}{% endblock %}
  </body>
</html>
